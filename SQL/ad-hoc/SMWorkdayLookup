WITH T1 AS (
    SELECT HR.BADGE_ID
         , HR.FULL_NAME
         , HR.TERMINATED
    FROM HR.T_EMPLOYEE AS HR
    WHERE HR.BADGE_ID IN
          ('107466', '95994', '96855', '115371', '119131', '115625', '203500', '201234', '98510', '122844', '106819',
           '200482', '200670', '116217', '119686', '200196', '60623', '117634', '201254', '1000377', '200633', '204915',
           '94945', '207102', '201094', '1000716', '119630', '120974', '201949')
)

SELECT *
FROM T1