CREATE OR REPLACE TEMPORARY TABLE D_POST_INSTALL.T_ER_ROOT_CAUSE_TAGS (
    SUBMITTED_DATETIME DATETIME,
    SUBMITTED_EMAIL TEXT,
    CASE_CREATED_DATETIME DATETIME,
    CASE_NUMBER INT,
    SERVICE_NUMBER TEXT,
    CASE_OWNER TEXT,
    ORIGIN TEXT,
    PRIORITY_BUCKET TEXT,
    ROOT_CAUSE_TAG TEXT
);

